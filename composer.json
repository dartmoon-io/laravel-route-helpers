{
    "name": "dartmoon/laravel-route-helpers",
    "description": "Set of helper for the Route facade",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dartmoon\\LaravelRouteHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Dartmoon",
            "email": "hello@dartmoon.io"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Dartmoon\\LaravelRouteHelper\\LaravelRouteHelperServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.24",
        "pestphp/pest": "^1.23",
        "pestphp/pest-plugin-laravel": "^1.4",
        "pestphp/pest-plugin-parallel": "^1.2"
    }
}
